<!-- Add this script just before the closing body tag -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    const blogContainer = document.querySelector('.blog .row');
    
    // Default articles to show if no news is cached
    const defaultArticles = [
        {
            title: "CDCI inks deal with the State Department of Housing and Development",
            description: "CDCI inks deal with the State Department of Housing and Development (SDHUD), to provide Fiscal Agency Services.",
            publishedAt: new Date(),
            source: { name: "CDCI News" },
            url: "#",
            image: "img/slum.jpg"
        },
        {
            title: "Consultancy Services - Alignment with NAIPs",
            description: "Consultancy Services - Alignment with National Agricultural Investment Plans (NAIPs) and Country Agribusiness Partnership Framework.",
            publishedAt: new Date(),
            source: { name: "Agriculture" },
            url: "#",
            image: "img/coffee.jpg"
        },
        {
            title: "CDCI Expands Its Workforce",
            description: "CDCI has recently increased its staff complement in response to its clients' needs.",
            publishedAt: new Date(),
            source: { name: "Company News" },
            url: "#",
            image: "img/staff.jpg"
        }
    ];

    // Try to get cached news articles
    const newsCache = localStorage.getItem('cdciNewsCache');
    let articlesToShow = defaultArticles;

    if (newsCache) {
        const {timestamp, articles} = JSON.parse(newsCache);
        const oneHourAgo = Date.now() - (60 * 60 * 1000);
        
        // Only use cached articles if they're less than 1 hour old and we have articles
        if (timestamp > oneHourAgo && articles && articles.length > 0) {
            articlesToShow = articles.slice(0, 3);
        }
    }

    // Always update the blog section with either cached or default articles
    updateBlogSection(articlesToShow);

    function updateBlogSection(articles) {
        if (!blogContainer) return;

        const blogHTML = articles.map((article, index) => {
            const delay = 0.2 + (index * 0.2);
            const category = getCategoryFromArticle(article);
            
            // Determine if this is an external news article or internal blog post
            const isExternalNews = article.url && article.url !== '#';
            const linkUrl = isExternalNews ? article.url : `blog-detail.html?title=${encodeURIComponent(article.title)}&category=${encodeURIComponent(category)}&image=${encodeURIComponent(article.image || getPlaceholderImage(category))}&description=${encodeURIComponent(article.description)}&date=${encodeURIComponent(article.publishedAt)}`;
            const linkTarget = isExternalNews ? '_blank' : '_self';
            
            return `
                <div class="col-lg-6 col-xl-4 wow fadeInUp" data-wow-delay="${delay}s">
                    <div class="blog-item">
                        <div class="blog-img">
                            <img src="${article.image || getPlaceholderImage(category)}" 
                                 alt="${article.title}"
                                 onerror="this.src='img/blog-1.png'">
                            <div class="blog-categiry py-2 px-4">
                                <span>${article.source.name}</span>
                            </div>
                        </div>
                        <div class="blog-content p-4">
                            <div class="blog-comment d-flex justify-content-between mb-3">
                                <div class="small">
                                    <span class="fa fa-calendar text-primary"></span> 
                                    ${new Date(article.publishedAt).toLocaleDateString()}
                                </div>
                            </div>
                            <a href="${linkUrl}" target="${linkTarget}" class="h4">
                                ${article.title}
                            </a>
                            <p>${article.description || ''}</p>
                            <a href="${linkUrl}" target="${linkTarget}" class="btn p-0">
                                Read More <i class="fa fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>
            `;
        }).join('');

        blogContainer.innerHTML = blogHTML;
    }

    function getCategoryFromArticle(article) {
        const title = (article.title || '').toLowerCase();
        const description = (article.description || '').toLowerCase();
        
        if (title.includes('governance') || description.includes('governance')) {
            return 'governance';
        } else if (title.includes('agriculture') || description.includes('agriculture')) {
            return 'agriculture';
        } else if (title.includes('water') || description.includes('water')) {
            return 'wash';
        } else if (title.includes('trade') || description.includes('trade')) {
            return 'trade';
        }
        return 'default';
    }

    function getPlaceholderImage(category) {
        const placeholders = {
            governance: "https://placehold.co/400x300/2B3B85/FFFFFF/png?text=Governance",
            agriculture: "https://placehold.co/400x300/3E7C17/FFFFFF/png?text=Agriculture",
            wash: "https://placehold.co/400x300/0096C7/FFFFFF/png?text=WASH",
            trade: "https://placehold.co/400x300/BC6C25/FFFFFF/png?text=Trade",
            default: "https://placehold.co/400x300/015FC9/FFFFFF/png?text=CDCI+Global"
        };
        return placeholders[category] || placeholders.default;
    }
});
</script>
</body>

</html>